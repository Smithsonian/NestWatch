{
  "name": "bootstrap4",
  "version": "1.0.0",
  "description": "Drupal theme to run with bootstrap 4.",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true scss/ -o css/",
    "build-task:autoprefixer": "postcss css/*.css --use autoprefixer -d css/",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'scss/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "repository": {
    "type": "git",
    "url": "VladimirAus@git.drupal.org:project/bootstrap4.git"
  },
  "author": "vladimir@tomato-elephant-studio.com",
  "license": "ISC",
  "homepage": "https://www.drupal.org/project/bootstrap4",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.5",
    "sass-lint": "^1.12.1",
    "slick": "^1.12.2",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "css-loader": "^1.0.1",
    "exports-loader": "^0.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "chokidar-cli": "^1.2.0",
    "node-sass-chokidar": "^0.0.1",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^4.0.0"
  }
}
