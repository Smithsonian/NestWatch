{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.21",
        "drupal/allowed_formats": "^1.1",
        "drupal/anchor_link": "^1.6",
        "drupal/backup_migrate": "^4.0",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/chosen": "^2.5",
        "drupal/coffee": "^1.0@beta",
        "drupal/components": "^1.0",
        "drupal/config_ignore": "^2.1",
        "drupal/console": "~1.0",
        "drupal/core": "^8.6.0",
        "drupal/crop": "~2.0",
        "drupal/devel": "^1.2",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_usage": "^2.0@alpha",
        "drupal/facets": "^1.0@beta",
        "drupal/field_group": "^1.0",
        "drupal/google_analytics": "^2.2",
        "drupal/honeypot": "^1.27",
        "drupal/image_widget_crop": "^2.1",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/ldap": "^3.0@beta",
        "drupal/linkit": "^4.3",
        "drupal/media_entity_audio": "~2.0",
        "drupal/media_entity_browser": "~2.0",
        "drupal/media_entity_video": "~2.0",
        "drupal/menu_block": "^1.5",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.7",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recreate_block_content": "^2.0",
        "drupal/svg_image": "^1.8",
        "drupal/taxonomy_menu_ui": "^1.0",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^1.9",
        "drupal/video_embed_field": "~2.0",
        "drupal/webform": "^5.0@RC",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.0.0",
        "harvesthq/chosen": "^1.8",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
